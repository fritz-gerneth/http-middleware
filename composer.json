{
  "name": "react-inspector/http-middleware",
  "description": "\ud83d\udd2d Measure HTTP requests going through an react/http",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com",
      "homepage": "https://www.wyrihaximus.net/"
    }
  ],
  "require": {
    "php": "^7.4",
    "psr/http-message": "^1.0",
    "react-inspector/metric": "^1.0",
    "react-inspector/metrics": "^1.0",
    "react/promise": "^2.7",
    "reactivex/rxphp": "^2.0"
  },
  "require-dev": {
    "ringcentral/psr7": "^1.2.2",
    "wyrihaximus/async-test-utilities": "^2.0"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ReactInspector\\HttpMiddleware\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReactInspector\\Tests\\HttpMiddleware\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
